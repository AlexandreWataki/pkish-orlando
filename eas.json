{
  "cli": {
    "version": ">= 11.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview-apk": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_WEATHER_API_KEY": "${EXPO_PUBLIC_WEATHER_API_KEY}",
        "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
